<!-- 优惠券创建 -->
<div class="modal fade" id="provideModal" tabindex="-1" role="dialog" aria-hidden="true" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog " role="document">
        <div class="modal-content">
            <form role="form" id="couponProvide">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title">优惠券发放</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-xs-12 MB10">
                            <span class="fb">基本信息</span>
                        </div>                                                                    
                    </div>
                    <div class="row">
                        <div class="col-xs-5 col-sm-5 MB10">
                            <span class="info-title">优惠券ID</span>
                            <span class="color-999" id="couponId"></span>
                        </div>
                        <div class="col-xs-5 col-sm-5 MB10">
                            <span class="info-title">类型</span>
                            <span class="color-999" id="couponType"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-5 col-sm-5 MB10">
                            <span class="info-title">名称</span>
                            <span class="color-999" id="couponName"></span>
                        </div>
                        <div class="col-xs-5 col-sm-5 MB10">
                            <span class="info-title">使用条件</span>
                            <span class="color-999" id="couponCondation"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12 MB10">
                            <span class="info-title">KEY</span>
                            <span class="color-999" id="couponKey"></span>
                        </div>                                                                    
                    </div>
                    <div class="row">
                        <div class="col-xs-5 col-sm-5 MB10">
                            <span class="info-title">减免期数</span>
                            <span class="color-999" id="couponTerm"></span>
                        </div>
                        <div class="col-xs-5 col-sm-5 MB10">
                            <span class="info-title">减免力度</span>
                            <span class="color-999" id="couponRate"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12 MB10">
                            <span class="info-title">有效时间</span>
                            <span class="color-999" id="couponAvailableTime">减免券</span>
                        </div>                                                                    
                    </div>
                    <div class="row">
                        <div class="col-xs-12 MB10">
                            <span class="info-title">说明</span>
                            <span class="color-999" id="couponDesc"></span>
                        </div>                                                                    
                    </div>
                    <div class="row">
                        <div class="col-xs-12 MB10">
                            <span class="info-title">备注</span>
                            <span class="color-999" id="couponComment"></span>
                        </div>                                                                    
                    </div>
                    <div class="row">
                        <div class="col-xs-12 MB10 MT10">
                            <span class="fb">发放信息</span>
                        </div>                                                                    
                    </div>
                    <div class="row">
                        <div class="col-xs-12 MB10">
                            <div class="form-inline">
                                <span class="info-title">业务线</span>
                                <select class="form-control" name="channel" id="channel">
                                </select>
                            </div>
                        </div>                                                                    
                    </div>
                    <div class="row">
                        <div class="col-xs-12 MB10">
                            <span class="info-title">发放名单</span>
                            <input type="file" id="fileSelect" accept=".xlsx">
                            <span class="ml-30 import-btn" id="template">(模板.xlsx)</span>
                        </div>                                                                     
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-info js-send-btn">确认发放</button>
                    <button type="button" class="btn btn-default js-cancel-btn">取消</button>                 
                </div>
            </form>
        </div>
    </div>
</div>
