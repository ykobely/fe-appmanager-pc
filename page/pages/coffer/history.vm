<!-- 页面历史 -->
<div class="modal fade" id="historyModal" tabindex="-1" role="dialog" aria-labelledby="historyModalLabel"  aria-hidden="true" data-keyboard="false" data-backdrop="static">
  	<div class="modal-dialog modal-md" role="document">
   	 	<div class="modal-content">
      		<form id="historyModalForm" >
            <input type="hidden" name="id">
            <input type="hidden" name="tableId">
      			<input type="hidden" name="checked">
      			<div class="modal-header">
			        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
			          <span aria-hidden="true">&times;</span>
			        </button>
			        <h4 class="modal-title" id="historyModalLabel">页面历史</h4>
			    </div>
		      	<div class="modal-body">
			        <div class="panel-body nopadding">
                        <table id="historyTable" class="table data-table table-border">
                      		<thead>
                        		<tr>
                                    <th>版本</th>
                					<th>操作人</th>
                					<th>操作时间</th>
                					<th>备注</th>
        		                </tr>
                      		</thead>
                    	</table>
			        </div>
		      	</div>
	      		<div class="modal-footer">
			        <button type="button" class="btn btn-primary" data-dismiss="modal">关闭</button>
			    </div>
      		</form>
    	</div>
  	</div>
</div>
